<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <style>
    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Light.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Light.woff) format("woff");
      font-style: normal;
      font-weight: 300;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Regular.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Regular.woff) format("woff");
      font-style: normal;
      font-weight: 400;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Medium.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Medium.woff) format("woff");
      font-style: normal;
      font-weight: 500;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Bold.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Bold.woff) format("woff");
      font-style: normal;
      font-weight: 700;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-LightItalic.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-LightItalic.woff) format("woff");
      font-style: italic;
      font-weight: 300;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Italic.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-Italic.woff) format("woff");
      font-style: italic;
      font-weight: 400;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-MediumItalic.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-MediumItalic.woff) format("woff");
      font-style: italic;
      font-weight: 500;
    }

    @font-face {
      font-family: Equinor;
      src: url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-BoldItalic.woff2) format("woff2"),
        url(//s478-cdne-edsartefacts-prod.azureedge.net/font/Equinor-BoldItalic.woff) format("woff");
      font-style: italic;
      font-weight: 700;
    }
  </style>
  <link rel="stylesheet" href="https://cdn.eds.equinor.com/font/equinor-font.css" />
  <link rel="icon" type="image/x-icon" href="/favicon.ico">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  @await RenderSectionAsync("Headers", required: false)

  <title>Project Portal</title>
</head>

<body>
  @RenderBody()

  @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
